{
  "name": "Corner Radius",
  "description": "Change the corner radius of a shape through quick keyboard shortcuts",
  "author": "David Williames",
  "homepage": "https://github.com/DWilliames/nudge-corner-radius",
  "version": 1.0,
  "identifier": "com.davidwilliames.corner-radius-sketch-plugin",
  "compatibleVersion": 37,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Increase corner radius",
      "identifier": "increaseRadius",
      "shortcut": "cmd ]",
      "script": "main.js",
      "handler": "increaseRadius"
    },
    {
      "name": "Increase corner radius (large nudge)",
      "identifier": "increaseRadiusLarge",
      "shortcut": "cmd shift ]",
      "script": "main.js",
      "handler": "increaseRadiusLarge"
    },
    {
      "name": "Decrease corner radius",
      "identifier": "decreaseRadius",
      "shortcut": "cmd [",
      "script": "main.js",
      "handler": "decreaseRadius"
    },
    {
      "name": "Decrease corner radius (large nudge)",
      "identifier": "decreaseRadiusLarge",
      "shortcut": "cmd shift [",
      "script": "main.js",
      "handler": "decreaseRadiusLarge"
    }
  ],
  "menu": {
    "items": [
      "increaseRadius",
      "decreaseRadius",
      "-",
      "increaseRadiusLarge",
      "decreaseRadiusLarge"
    ]
  }
}
